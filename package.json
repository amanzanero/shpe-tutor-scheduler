{
  "name": "shpe-tutor-scheduler",
  "author": "Andrew Manzanero",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Webapp for SHPE Students at studynights!",
  "main": "index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=production npm run build",
    "start": "cross-env NODE_ENV=production node server",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test:client": "react-scripts test",
    "test:server": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --exit",
    "eject": "react-scripts eject",
    "lint:client": "./node_modules/.bin/eslint src/*.js",
    "lint:server": "./node_modules/.bin/eslint server/*.js",
    "server": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --watch server server",
    "test": "npm run test:server && npm run test:client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.2",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "cross-env": "^5.2.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.11.0",
    "http-status": "^1.0.1",
    "is-mobile": "^2.0.1",
    "joi": "^14.3.1",
    "joi-objectid": "^2.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.7.5",
    "mongoose-error-handler": "^1.0.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.2.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.0.1",
    "mocha": "^10.2.0",
    "nodemon": "^1.14.12",
    "prettier": "^1.18.2"
  }
}
